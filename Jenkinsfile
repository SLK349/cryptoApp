pipeline {    agent any    stages {        stage('Checkout') {            steps {                git branch: 'main', url: 'https://github.com/SLK349/cryptoApp.git'            }        }        stage('Build') {            steps {                sh 'npm install'            }        }        stage('Test') {            steps {                sh 'npm test'            }        }    }    post {        always{            junit 'cryptoApp/crypto-api/test-results/*.xml'        }        success {            echo 'Pipeline succeeded!'        }        failure {            echo 'Pipeline failed!'        }    }}