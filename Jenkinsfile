pipeline {    agent any    stages {        stage('Checkout') {            steps {                git branch: 'main', url: 'https://gitlab.com/lascaudsim/crypto-app'            }        }        stage('Build') {            steps {                sh 'npm install'            }        }        stage('Test') {            steps {                sh 'npm test'            }        }    }    post {        success {            echo 'Pipeline succeeded!'        }        failure {            echo 'Pipeline failed!'        }    }}